public class Solution {
    public void ReverseString(char[] s) {
      char temp;
      int mid = s.Length / 2;
      int last_index = s.Length - 1;
      for(int i = 0; i < mid ; i++)
      {
          temp = s[i];
          s[i]= s[last_index - i];
          s[last_index - i] = temp;
      }
    }
}
